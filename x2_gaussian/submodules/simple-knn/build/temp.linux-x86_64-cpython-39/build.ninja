ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-11.7/bin/nvcc

cflags = -pthread -B /data/whyu/anaconda3/envs/x2gs/compiler_compat -Wl,--sysroot=/ -Wno-unused-result -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /data/whyu/anaconda3/envs/x2gs/include -fPIC -O2 -isystem /data/whyu/anaconda3/envs/x2gs/include -fPIC -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/TH -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda-11.7/include -I/data/whyu/anaconda3/envs/x2gs/include/python3.9 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/TH -I/data/whyu/anaconda3/envs/x2gs/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda-11.7/include -I/data/whyu/anaconda3/envs/x2gs/include/python3.9 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/build/temp.linux-x86_64-cpython-39/ext.o: compile /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/ext.cpp
build /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/build/temp.linux-x86_64-cpython-39/simple_knn.o: cuda_compile /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/simple_knn.cu
build /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/build/temp.linux-x86_64-cpython-39/spatial.o: cuda_compile /data/whyu/x2gs_code/x2_gaussian/submodules/simple-knn/spatial.cu







